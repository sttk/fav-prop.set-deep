{
  "name": "@fav/prop.set-deep",
  "version": "1.0.2",
  "description": "Sets a property value in property tree of an object with a property path.",
  "main": "index.js",
  "files": [
    "index.js",
    "web/"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "mocha",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm test",
    "coveralls": "nyc --reporter=text-lcov npm test | coveralls",
    "web:build": "npm run web:build-src && npm run web:build-test",
    "web:build-src": "browserify index.js --standalone fav.prop.set-deep -o web/fav.prop.set-deep.js && cd web && uglifyjs fav.prop.set-deep.js --compress --mangle -o fav.prop.set-deep.min.js --source-map url=fav.prop.set-deep.min.js.map",
    "web:build-test": "node tool/mktest.js",
    "chrome:install": "npm i --no-save mocha-chrome",
    "chrome:test": "mocha-chrome test/web/browser-test.html",
    "phantom:install": "npm i --no-save phantomjs-prebuilt mocha-phantomjs",
    "phantom:test": "mocha-phantomjs -p node_modules/.bin/phantomjs test/web/browser-test.html",
    "build": "npm run lint && npm run coverage && npm run web:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sttk/fav-prop.set-deep.git"
  },
  "keywords": [
    "set",
    "deep",
    "object",
    "prop",
    "fav"
  ],
  "author": "Takayuki Sato",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sttk/fav-prop.set-deep/issues"
  },
  "homepage": "https://github.com/sttk/fav-prop.set-deep#readme",
  "dependencies": {
    "@fav/type.is-array": "^1.0.2"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "chai": "^3.5.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.19.0",
    "mocha": "^3.4.2",
    "nyc": "^15.1.0",
    "uglify-js": "^3.12.7"
  }
}
